Algoritmo "semnome"

Var
VE, VF, parcelas, VD: real


Inicio
      escreva("Qual o valor do empréstimo do SR(a)? ")
      leia(VE)
      escreva("Quantas parcelas o SR(a) ira fazer?" )
      leia(parcelas)
      VD <- ((VE*120)/100))
      VF <- (VD/parcelas)
      escreva("O valor de cada parcela ficou: ", VF)
      


Fimalgoritmo